<!doctype html>
<html>
<head>
<meta charset="utf-8">
<title> PNG转ICO,PNG转ICNS | 图标格式在线转换 - EASYICON.NET</title>
<link href="https://cdn-img.easyicon.net/style/complus.css" rel="stylesheet" type="text/css" />
<link href="https://cdn-img.easyicon.net/style/custom_v3.css" rel="stylesheet" type="text/css" />
<link rel="shortcut icon" href="https://cdn-img.easyicon.net/favicon.ico" />
<SCRIPT language="JavaScript" src="https://cdn-img.easyicon.net/js/tabcontent.js" type="text/JavaScript"></SCRIPT>
<script type="text/javascript">
function checkerror(){
 var curUrl = document.getElementById('url').value;
 var ft=document.getElementById("format");
 var sz=document.getElementById("size");
 var isTxt = ".png";
	if(curUrl.indexOf(isTxt)=0){
	   alert("远程地址格式错误，只支持PNG格式。");
	   document.getElementById('url').focus();
	   return false;
	}

	if(ft.value==""){
	alert("请选择目标格式");
	ft.focus();
	return false;
	}
	
	if(sz.value==""){
	alert("请选择目标尺寸");
	sz.focus();
	return false;
	}
}
</script>
</head>
<body>
<div class="vector_bg">
<div style="clear:both;padding:40px 0px 0px 200px; text-align:left">
<div class="back_index">
<a href="/">返回首页</a>
</div>
<div class="info" id="info">
<div style="height:50px;width:50px;"></div>
</div>
<div class="tab_menu">
<ul id="tabs">
<li><a href="javascript:;" rel="tcontent2">从远程地址制作图标</a></li>
<li><a href="javascript:;" rel="tcontent3">上传PNG制作图标</a></li>
</ul>
</div>
<div class="layout_item">
<div id="tcontent2" class="tabcontent">
<form name="urlconvert" method="post" action="/covert/" onSubmit="return checkerror()" enctype="multipart/form-data">
<input type="hidden" value="url" name="upload_type" id="upload_type" />
<input type="text" name="url" id="url" value="http://" class="textwidth" /> <span class="grey">只支持PNG格式</span>
<div class="pdclass">
<select name="format" id="format" class="selectwidth">
<option value="">图标格式</option>
<option value="icon">Microsoft icon</option><option value="icns">MacOs icon</option></select>
</div>
<div class="pdclass">
<select name="width" id="size" class="selectwidth">
<option value="">图标尺寸</option>
<option value="16">16x16</option><option value="32">32x32</option><option value="48">48x48</option><option value="64">64x64</option><option value="96">96x96</option><option value="128">128x128</option><option value="256">256x256</option><option value="512">512x512</option></select>
</div>
<div class="pdclass">
<input type="submit" name="convert" class="btn btn-greenx" value="开始转换" />
</div>
</form>
</div>
<div id="tcontent3" class="tabcontent">
<form name="fileconvert" method="post" action="/covert/" enctype="multipart/form-data">
<input type="hidden" value="upload_file" name="upload_type" id="upload_type" />
<input type="file" id="file" name="file" />
<div class="pdclass smallfont">
Size(16x16, 32x32, 48x48, 64x64, 96x96, 128x128, 256x256, 512x512)</div>
<div class="pdclass">
<select name="format" class="selectwidth">
<option value="">图标格式</option>
<option value="icon">Microsoft icon</option><option value="icns">MacOs icon</option></select>
</div>
<div class="pdclass">
<select name="width" class="selectwidth">
<option value="">图标尺寸</option>
<option value="16">16x16</option><option value="32">32x32</option><option value="48">48x48</option><option value="64">64x64</option><option value="96">96x96</option><option value="128">128x128</option><option value="256">256x256</option><option value="512">512x512</option></select>
</div>
<div class="pdclass">
<input type="submit" name="convert" class="btn btn-greenx" value="开始转换" />
</div>
</form>
<div class="footer">
</div>
</div>
</div>
